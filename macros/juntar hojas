Sub Macro1()
'
' Macro1 Macro
    Sheets("diseño").Select
    Range("A2:C3").Select
    Selection.Copy
    Sheets("obras").Select
    Range("A4").Select
    ActiveSheet.Paste
    Range("F6").Select
End Sub


version de respaldo

Sub Macro1()
'
' Macro1 Macro
'

'
    Range("A2:C3").Select
    Range("C3").Activate
    Selection.Copy
    Sheets("obras").Select
    Range("A4").Select
    ActiveSheet.Paste
    Range("G5").Select
End Sub


version corregida

Sub Macro1()
'
' Macro1 Macro
'

'
    Sheets("diseño").Select
    Selection.Copy
    Sheets("obras").Select
    Range("A4").Select
    ActiveSheet.Paste
    Range("H12").Select
End Sub



